record(calcout, "$(P)DI0_calc") {
  field(PINI, "YES")
  field(CALC, "(A >> 0) & 1")
  field(INPA, "$(P)FIO CP NMS")
  field(OUT, "$(P)DI0 PP")
  field(PREC, "0")
}
record(bi, "$(P)DI0") {
  field(DESC, "Ch 0 Dig In")
}

