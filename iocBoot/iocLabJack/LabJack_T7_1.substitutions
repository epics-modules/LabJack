{R,                              PORT,         OFFSET}
{LJT7:1:AIResolution0,   LJT7_1_AIResolution,       0}
{LJT7:1:AIResolution1,   LJT7_1_AIResolution,       2}
{LJT7:1:AIResolution2,   LJT7_1_AIResolution,       4}
{LJT7:1:AIResolution3,   LJT7_1_AIResolution,       6}
{LJT7:1:AIResolution4,   LJT7_1_AIResolution,       8}
{LJT7:1:AIResolution5,   LJT7_1_AIResolution,      10}
{LJT7:1:AIResolution6,   LJT7_1_AIResolution,      12}
{LJT7:1:AIResolution7,   LJT7_1_AIResolution,      14}
{LJT7:1:AIResolution8,   LJT7_1_AIResolution,      16}
{LJT7:1:AIResolution9,   LJT7_1_AIResolution,      18}
{LJT7:1:AIResolution10,  LJT7_1_AIResolution,      20}
{LJT7:1:AIResolution11,  LJT7_1_AIResolution,      22}
{LJT7:1:AIResolution12,  LJT7_1_AIResolution,      24}
{LJT7:1:AIResolution13,  LJT7_1_AIResolution,      26}
}

# Set S.E. / Diff for each even number channel individually
# Single-Ended (199, default) or Differential (1)
file "$(LABJACK)/db/LabJack_T7_seDiff.template" { pattern
{R,                           PORT,             ONVL, OFFSET}
{LJT7:1:seDiff_Ch0,   LJT7_1_AIdiff_Out_Word,    1,       0}
{LJT7:1:seDiff_Ch2,   LJT7_1_AIdiff_Out_Word,    3,       2}
{LJT7:1:seDiff_Ch4,   LJT7_1_AIdiff_Out_Word,    5,       4}
{LJT7:1:seDiff_Ch6,   LJT7_1_AIdiff_Out_Word,    7,       6}
{LJT7:1:seDiff_Ch8,   LJT7_1_AIdiff_Out_Word,    9,       8}
{LJT7:1:seDiff_Ch10,  LJT7_1_AIdiff_Out_Word,   11,      10}
{LJT7:1:seDiff_Ch12,  LJT7_1_AIdiff_Out_Word,   13,      12}
}

# Analog outputs done with word access.
file "$(LABJACK)/db/LabJack_T7_ao.template" { pattern
{R,              PORT,  OFFSET, PREC, LOPR, HOPR, DRVL, DRVH,    DESC}
{LJT7:1:AO0,  LJT7_1_AO,   0,    4,    0,     5,   0,     5, "Ch 0 Ana Out"}
{LJT7:1:AO1,  LJT7_1_AO,   2,    4,    0,     5,   0,     5, "Ch 1 Ana Out"}

# These are for the LJTick-DAC modules
{LJT7:1:AO2,  LJT7_1_AO_TICK,   0,    4,  -10,    10,  -10,   10, "Ch 2 Ana Out"}
{LJT7:1:AO3,  LJT7_1_AO_TICK,   2,    4,  -10,    10,  -10,   10, "Ch 3 Ana Out"}
#{LJT7:1:AO4,  LJT7_1_AO_TICK,  24,    4,  -10,    10,  -10,   10, "Ch 4 Ana Out"}
#{LJT7:1:AO5,  LJT7_1_AO_TICK,  26,    4,  -10,    10,  -10,   10, "Ch 5 Ana Out"}
#{LJT7:1:AO6,  LJT7_1_AO_TICK,  28,    4,  -10,    10,  -10,   10, "Ch 6 Ana Out"}
#{LJT7:1:AO7,  LJT7_1_AO_TICK,  30,    4,  -10,    10,  -10,   10, "Ch 7 Ana Out"}
#{LJT7:1:AO8,  LJT7_1_AO_TICK,  32,    4,  -10,    10,  -10,   10, "Ch 8 Ana Out"}
#{LJT7:1:AO9,  LJT7_1_AO_TICK,  34,    4,  -10,    10,  -10,   10, "Ch 9 Ana Out"}
#{LJT7:1:AO8,  LJT7_1_AO_TICK,  36,    4,  -10,    10,  -10,   10, "Ch 10 Ana Out"}
#{LJT7:1:AO9,  LJT7_1_AO_TICK,  38,    4,  -10,    10,  -10,   10, "Ch 11 Ana Out"}
}

# Digital Inputs
file "$(LABJACK)/db/LabJack_T7_di.template" { pattern
{R}
{LJT7:1:}
}

