

# The LabJack T7 has 4 banks of digital I/O, set as either intputs or
# outputs at boot time.
# FIO = DIO0 - 7,   initially set to inputs,  EPICS channels BI0 - BI7
# EIO = DIO8 - 15,  initially set to outputs, EPICS channels BO0 - BO7
# CIO = DIO16 - 19, initially set to inputs,  EPICS channels BI8 - BI11
# MIO = DIO20 - 23, initially set to outputs, EPICS channels BO8 - BO10
#
# Note, you may change this configuration but "LabJack_T7.cmd" will need to be modified
#       accordingly, and possibly the displays.
#
# Digital inputs done with word access.  FIO is the first 8 digital inputs, CIO is the next 4 inputs.
file "$(LABJACK)/db/LabJack_T7_longinInt32.template" { pattern
{R,                PORT,     OFFSET,   DATA_TYPE,      SCAN}
{LJT7:1:FIO, LJT7_1_FIO_In_Word, 0, UINT16, "I/O Intr"}
{LJT7:1:CIO, LJT7_1_CIO_In_Word, 0, UINT16, "I/O Intr"}
}

# Digital outputs.  EIO is the second 8 digital channels, used as Outputs.
file "$(LABJACK)/db/LabJack_T7_bo_bit.template" { pattern
{R,                PORT,       OFFSET,  ZNAM, ONAM,      DESC}
{LJT7:1:DO0, LJT7_1_EIO_Out_Word,   0,  Low,  High, "Ch 0 Dig Out"}
{LJT7:1:DO1, LJT7_1_EIO_Out_Word,   1,  Low,  High, "Ch 1 Dig Out"}
{LJT7:1:DO2, LJT7_1_EIO_Out_Word,   2,  Low,  High, "Ch 2 Dig Out"}
{LJT7:1:DO3, LJT7_1_EIO_Out_Word,   3,  Low,  High, "Ch 3 Dig Out"}
{LJT7:1:DO4, LJT7_1_EIO_Out_Word,   4,  Low,  High, "Ch 4 Dig Out"}
{LJT7:1:DO5, LJT7_1_EIO_Out_Word,   5,  Low,  High, "Ch 5 Dig Out"}
{LJT7:1:DO6, LJT7_1_EIO_Out_Word,   6,  Low,  High, "Ch 6 Dig Out"}
{LJT7:1:DO7, LJT7_1_EIO_Out_Word,   7,  Low,  High, "Ch 7 Dig Out"}
# MIO channels
{LJT7:1:DO8,  LJT7_1_MIO_Out_Word,  0,  Low,  High, "Ch 8 Dig Out"}
{LJT7:1:DO9,  LJT7_1_MIO_Out_Word,  1,  Low,  High, "Ch 9 Dig Out"}
{LJT7:1:DO10, LJT7_1_MIO_Out_Word,  2,  Low,  High, "Ch 10 Dig Out"}
}

# Analog Inputs done with word access.
file "$(LABJACK)/db/LabJack_T7_ai.template" { pattern
{R,              PORT,     OFFSET,   SCAN,  PREC,     DESC}
{LJT7:1:AI0,  LJT7_1_AI_In,  0,  ".1 second", 4, "Ch 0 Ana In"}
{LJT7:1:AI1,  LJT7_1_AI_In,  2,  ".1 second", 4, "Ch 1 Ana In"}
{LJT7:1:AI2,  LJT7_1_AI_In,  4,  ".1 second", 4, "Ch 2 Ana In"}
{LJT7:1:AI3,  LJT7_1_AI_In,  6,  ".1 second", 4, "Ch 3 Ana In"}
{LJT7:1:AI4,  LJT7_1_AI_In,  8,  ".1 second", 4, "Ch 4 Ana In"}
{LJT7:1:AI5,  LJT7_1_AI_In,  0,  ".1 second", 4, "Ch 5 Ana In"}
{LJT7:1:AI6,  LJT7_1_AI_In,  2,  ".1 second", 4, "Ch 6 Ana In"}
{LJT7:1:AI7,  LJT7_1_AI_In, 14,  ".1 second", 4, "Ch 7 Ana In"}
{LJT7:1:AI8,  LJT7_1_AI_In, 16,  ".1 second", 4, "Ch 8 Ana In"}
{LJT7:1:AI9,  LJT7_1_AI_In, 18,  ".1 second", 4, "Ch 9 Ana In"}
{LJT7:1:AI10, LJT7_1_AI_In, 20,  ".1 second", 4, "Ch 10 Ana In"}
{LJT7:1:AI11, LJT7_1_AI_In, 22,  ".1 second", 4, "Ch 11 Ana In"}
{LJT7:1:AI12, LJT7_1_AI_In, 24,  ".1 second", 4, "Ch 12 Ana In"}
{LJT7:1:AI13, LJT7_1_AI_In, 26,  ".1 second", 4, "Ch 13 Ana In"}
}

file "$(LABJACK)/db/LabJack_T7_aiRange.template" { pattern
{R,                    PORT,         OFFSET}
{LJT7:1:AIRange0,   LJT7_1_AIRange,       0}
{LJT7:1:AIRange1,   LJT7_1_AIRange,       2}
{LJT7:1:AIRange2,   LJT7_1_AIRange,       4}
{LJT7:1:AIRange3,   LJT7_1_AIRange,       6}
