{LJT7:1:AIRange6,   LJT7_1_AIRange,      12}
{LJT7:1:AIRange7,   LJT7_1_AIRange,      14}
{LJT7:1:AIRange8,   LJT7_1_AIRange,      16}
{LJT7:1:AIRange9,   LJT7_1_AIRange,      18}
{LJT7:1:AIRange10,  LJT7_1_AIRange,      20}
{LJT7:1:AIRange11,  LJT7_1_AIRange,      22}
{LJT7:1:AIRange12,  LJT7_1_AIRange,      24}
{LJT7:1:AIRange13,  LJT7_1_AIRange,      26}
}

